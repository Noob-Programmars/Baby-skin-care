<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".foodpage">

    <ImageButton
        android:id="@+id/babyfoodbutton"
        android:layout_width="89dp"
        android:layout_height="81dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="44dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/babymilkButton"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/babymedicineButton"
        app:srcCompat="@drawable/babyfood" />

    <ImageButton
        android:id="@+id/babymilkButton"
        android:layout_width="89dp"
        android:layout_height="81dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="44dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/milkbottle" />

    <ImageButton
        android:id="@+id/babymedicineButton"
        android:layout_width="89dp"
        android:layout_height="81dp"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="44dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/medicine" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="281dp"
        android:layout_height="282dp"
        android:layout_marginTop="260dp"
        android:layout_marginEnd="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baby" />

    <ProgressBar
        android:id="@+id/hungerfoodbar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="251dp"
        android:layout_height="61dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="80dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="31dp"
        android:layout_height="31dp"
        android:background="@drawable/rounder_textview"
        android:gravity="center"
        android:text="3"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="606dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="31dp"
        android:layout_height="31dp"
        android:background="@drawable/rounder_textview"
        android:gravity="center"
        android:text="3"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="279dp"
        tools:layout_editor_absoluteY="606dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="31dp"
        android:layout_height="31dp"
        android:background="@drawable/rounder_textview"
        android:gravity="center"
        android:text="3"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="606dp" />

</androidx.constraintlayout.widget.ConstraintLayout>